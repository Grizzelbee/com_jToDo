<?xml version="1.0" encoding="utf-8"?><form><!--Project : jTODO for JoomlaFilename: /admin/models/forms/notification.xmlVersion : 2.1.0--><field  name = "id"        type = "hidden"/><!-- Felder: name, ordering, published --><field    name        = "fk_category"    type        = "categories"    label       = "COM_JTODO_CATEGORY"    description = "COM_JTODO_CATEGORY_DESC"    default     = "0"    class       = "inputbox"    size        = "1"/><field    name        = "fk_juserid"    type        = "user"    label       = "COM_TODO_JUSER"    description = "COM_TODO_JUSER_DESC"    class       = "inputbox"    size        = "60"    default     = "0"/><field    name        = "ordering"    type        = "text"    label       = "COM_JTODO_ORDERING"    description = "COM_JTODO_ORDERING_DESC"    default     = "0"    class       = "inputbox"></field><field    name        = "published"    type        = "radio"    label       = "COM_JTODO_PUBLISHED"    description = "COM_JTODO_PUBLISHED_DESC"    default     = "1"    class       = "radio btn-group">    <option value="1">JYES</option>    <option value="0">JNO</option></field></form>
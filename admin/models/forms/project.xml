<?xml version="1.0" encoding="utf-8"?><form><!--Project : jTODO for JoomlaFilename: /admin/models/forms/category.xmlVersion : 1.1.1--><field  name = "id"        type = "hidden"/><!-- Felder: name, ordering, published --><field  name        = "name"        type        = "text"        label       = "COM_JTODO_PROJECT_NAME"        description = "COM_JTODO_PROJECT_NAME_DESC"        class       = "inputbox"        size        = "128"        required    = "true"/><field    name        = "ordering"    type        = "text"    label       = "COM_JTODO_ORDERING"    description = "COM_JTODO_ORDERING_DESC"    default     = "0"    class       = "inputbox"/><field    name        = "published"    type        = "radio"    label       = "COM_JTODO_PUBLISHED"    description = "COM_JTODO_PUBLISHED_DESC"    default     = "1"    class       = "radio btn-group">    <option value="1">JYES</option>    <option value="0">JNO</option></field>&lt; br /&gt;<field  name        = "preamble"        type        = "editor"        label       = "COM_JTODO_PROJECT_PREAMBLE"        description = "COM_JTODO_PROJECT_PREAMBLE_DESC"        class       = "inputbox"        size        = "128"        required    = "false"/></form>